{
  "permissions": {
    "function": {
      "agentLambda": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ExternalLayer",
      "arn": "arn:aws:lambda:us-east-2:553035198032:layer:git-lambda2:8"
    },
    {
      "type": "ProjectLayer",
      "resourceName": "terrascanbackendcontrollerLayer",
      "env": "dev",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "pguser",
      "environmentVariableName": "pguser"
    },
    {
      "cloudFormationParameterName": "pghost",
      "environmentVariableName": "pghost"
    },
    {
      "cloudFormationParameterName": "pgpassword",
      "environmentVariableName": "pgpassword"
    },
    {
      "cloudFormationParameterName": "pgport",
      "environmentVariableName": "pgport"
    },
    {
      "cloudFormationParameterName": "pgdatabase",
      "environmentVariableName": "pgdatabase"
    }
  ]
}